generator client {
  provider = "prisma-client-js"
  output   = "src/app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  companyName  String
  purpose      String
  contactEmail String
  website      String?
  walletAddress String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}